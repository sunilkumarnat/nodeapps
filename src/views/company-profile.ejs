<%- include('includes/header') %>
    <%- include('includes/navbar') %>

        <div class="container">
            <div class="row">
                <div class="col-sm-12 mb-5 text-center">
                    <img src="/images/logos/<%= companyProfileData.logo %>" class="circle profile-logo mb-3 mt-3" />
                    <h5 class="card-title mb-2">
                        <%= companyProfileData.company_name %>
                    </h5>
                    <h6 class="card-subtitle mb-2 text-muted">
                        <%= companyProfileData.about_company %>
                    </h6>
                    <p class="card-subtitle mb-2 text-muted">
                        <%= companyProfileData.contact_no %>
                    </p>
                    <p class="card-subtitle mb-2 text-muted">
                        <%= companyProfileData.email %>
                    </p>
                    <p class="card-subtitle mb-2 text-muted">
                        <%= companyProfileData.address %>
                            <%= companyProfileData.city %>
                                <%= companyProfileData.state %>
                                    <%= companyProfileData.pincode %>
                    </p>
                </div>
                <div class="col-sm-12 mb-3">
                    <div class="card bg-light">
                        <div class="card-header">
                            <h5 class="card-title">
                                <%= companyProfileData.company_name %>
                            </h5>
                            <h6 class="card-subtitle mb-2 text-muted">
                                <%= companyProfileData.email %>
                            </h6>
                        </div>
                        <div class="card-body p-3">

                        </div>
                    </div>
                </div>
            </div>
        </div>




        <%- include('includes/footer') %>